<Project>
  <PropertyGroup>
    <WpfTest>true</WpfTest>
  </PropertyGroup>
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)../'))" />
  <PropertyGroup>
    <SignAssembly Condition="'$(SignAssembly)'!=''">true</SignAssembly>
    <IsShipping>false</IsShipping>
    <EnableXlfLocalization>false</EnableXlfLocalization>
    <CLSCompliant>false</CLSCompliant>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <EmbeddedResource>
      <GenerateSource>false</GenerateSource>
    </EmbeddedResource>
  </ItemDefinitionGroup>
  <Import Project="$(WpfTestsDir)MultiTargeting.props" />

  <PropertyGroup>
    <InternalUtilitiesProject>$(WpfTestsDir)Infra\TestUtilities\InternalUtilities\InternalUtilities.csproj</InternalUtilitiesProject>
    <TestContractsProject>$(WpfTestsDir)Infra\CommunityChest\TestContracts\TestContracts.csproj</TestContractsProject>
    <TestApiCoreProject>$(WpfTestsDir)Infra\TestApi\TestApiCore\Code\TestApiCore.csproj</TestApiCoreProject>
    <TestRuntimeProject>$(WpfTestsDir)Common\TestRuntime\TestRuntime.csproj</TestRuntimeProject>
    <TestServicesProject>$(WpfTestsDir)Common\Drt\TestServices\TestServices.csproj</TestServicesProject>
    <QualityVaultUtilitiesProject>$(WpfTestsDir)Infra\QualityVault\QualityVaultUtilities\QualityVaultUtilities.csproj</QualityVaultUtilitiesProject>
    <DrtToolsProject>$(WpfTestsDir)Common\DRT\Tools\DrtTools.csproj</DrtToolsProject>
  </PropertyGroup>

</Project>